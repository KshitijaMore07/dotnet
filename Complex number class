using System;

class Complex
{
    int real, imag;

    public Complex(int r, int i)
    {
        real = r;
        imag = i;
    }

    public void Add(Complex c)
    {
        Console.WriteLine((real + c.real) + " + " + (imag + c.imag) + "i");
    }
}

class Program
{
    static void Main()
    {
        Complex c1 = new Complex(2, 3);
        Complex c2 = new Complex(4, 5);
        c1.Add(c2);
    }
}
