using System;

class Program
{
    static void Main()
    {
        int n;
        long fact = 1;

        Console.WriteLine("Enter a number:");
        n = Convert.ToInt32(Console.ReadLine());

        for (int i = 1; i <= n; i++)
        {
            fact = fact * i;
        }

        Console.WriteLine("Factorial is: " + fact);
    }
}
