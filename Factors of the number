using System;

class Program
{
    static void Main()
    {
        int n;

        Console.WriteLine("Enter a number:");
        n = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine("Factors of " + n + " are:");

        for (int i = 1; i <= n; i++)
        {
            if (n % i == 0)
            {
                Console.Write(i + " ");
            }
        }
    }
}
