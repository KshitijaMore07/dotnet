using System;

class Program
{
    static void Main()
    {
        int n, a = 0, b = 1, c;

        Console.WriteLine("Enter number of terms:");
        n = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine("Fibonacci Series:");

        for (int i = 1; i <= n; i++)
        {
            Console.Write(a + " ");
            c = a + b;
            a = b;
            b = c;
        }
    }
}
